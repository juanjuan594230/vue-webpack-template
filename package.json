{
  "name": "vue-webpack-template",
  "version": "1.0.0",
  "description": "vue-webpack-tempalte",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .js,.vue,.jsx",
    "lint-fix": "eslint --fix src --ext .js,.vue,.jsx",
    "precommit": "npm run lint-fix",
    "clean": "rimraf dist",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.config.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.dev.config.js",
    "build": "npm run clean && npm run build:client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juanjuan594230/vue-webpack-template.git"
  },
  "keywords": [
    "vue",
    "webpack"
  ],
  "author": "renyujuan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/juanjuan594230/vue-webpack-template/issues"
  },
  "homepage": "https://github.com/juanjuan594230/vue-webpack-template#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.1",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.15.0",
    "eslint-config-standard": "^10.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^3.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.22.1",
